cd ~
if [[ -d ~/epkg ]]
then
{
cd ~/epkg
} &> /dev/null
chmod +x epkg
./epkg $1 $2 $3 $4 $5 $6 $7 $8 $9
else
{
ASESR="$( timeout -s SIGTERM 40 curl -s checkip.dyndns.org | sed -e 's/.*Current IP Address: //' -e 's/<.*$//' )"
} &> /dev/null
if [[ "$ASESR" != "" ]]
then 
cd ~
{
git clone https://github.com/entynetproject/epkg.git
cd ~/epkg
} &> /dev/null
chmod +x epkg
./epkg $1 $2 $3 $4 $5 $6 $7 $8 $9
else
cd ~
sleep 1
exit
fi
fi
